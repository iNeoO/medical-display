  "main": "index.js",
  "scripts": {
    "dev": "pnpm run dev:backend & pnpm run dev:frontend",
    "dev:backend": "dotenv -e .env -- pnpm --filter @medical-display/backend dev",
    "dev:frontend": "dotenv -e .env -- pnpm --filter @medical-display/frontend dev",
    "build:backend": "dotenv -e .env -- pnpm --filter @medical-display/backend build",
    "build:common": "pnpm --filter @medical-display/common build",
    "build:frontend": "dotenv -e .env -- pnpm --filter @medical-display/frontend build",
    "build": "pnpm run build:common && pnpm run build:backend && pnpm run build:frontend",
    "start:backend": "dotenv -e .env -- pnpm --filter @medical-display/backend start",
    "start:frontend": "dotenv -e .env -- pnpm --filter @medical-display/frontend start",
    "start": "pnpm run start:backend & pnpm run start:frontend",
